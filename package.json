{
  "name": "my-nanny-api",
  "version": "1.0.0",
  "description": "Voice controlled chores application with Alexa",
  "main": "server/src/server.js",
  "scripts": {
    "start": "nodemon -w server/src --exec \"babel-node server/src/server.js --presets es2015,stage-0\"",
    "test": "./node_modules/.bin/mocha \"server/test/**/*Spec.js\" --compilers js:babel-core/register --reporter spec",
    "doc": "./node_modules/.bin/jsdoc -c ./jsdoc.json -r -d ./server/src/docs && npm run deployDocs",
    "deployDocs": "git add server/src/docs/ && git commit -m 'Update docs' && git push heroku master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dystopian-smurfs/my-nanny-api.git"
  },
  "author": "Students at Hack Reactor 2016",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dystopian-smurfs/my-nanny-api/issues"
  },
  "homepage": "https://github.com/dystopian-smurfs/my-nanny-api#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.15.2",
    "eslint-config-hackreactor": "github:hackreactor-labs/eslint-config-hackreactor",
    "express": "^4.14.0",
    "jsdoc": "^3.4.3",
    "mysql": "^2.12.0",
    "nodemon": "^1.11.0",
    "sequelize": "^3.25.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "mocha": "^3.1.2",
    "supertest": "^2.0.1"
  }
}
